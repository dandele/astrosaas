---
  import BaseLayout from "../layouts/BaseLayout.astro";
  import Navigation from "../components/global/Navigation.astro";
  import Hero from "../components/landing/Hero-agency-os.astro";
  import Team from "../components/landing/Team.astro";
  import Footer from "../components/global/Footer.astro";
  import Banner from "../components/global/Banner.astro";
  import Features from "../components/landing/agency_os-features.astro";
  import Vantaggi from "../components/landing/agency_os-vantaggi.astro";
  import Timer from "../components/landing/agency_os-timer.astro";
  import Pricing from "../components/landing/agency_os-pricing.astro";
  import Countdown from "../components/landing/agency_os-countdown.astro";
  import Intro from "../components/landing/agency_os-intro.astro";
  import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo title="Notion Builders - Agency OS" description="Gestisci la tua Digital Agency con un unico sistema"
  canonical="https://www.notionbuilders.it/agency-os" openGraph={{
  url: "https://www.notionbuilders.it/agency-os" , title: "Notion Builders - Agency OS" ,
  description: "Gestisci la tua Digital Agency con un unico sistema!" , images: [ { url: "https://www.notionbuilders.it/agency-os-hero.webp" ,
  width: 1200, height: 630, alt: "Social open graph" , type: "image/webp" , }, {
  url: "https://www.notionbuilders.it/agency-os-hero.webp" , width: 4096, height: 4096, alt: "Twitter open graph" ,
  type: "image/webp" , }, { url: "https://www.notionbuilders.it/agency-os-hero.webp" }, {
  url: "https://www.notionbuilders.it/agency-os-hero.webp" }, ], site_name: "Notion Builders" , }} twitter={{ handle: "" ,
  site: "@site" , cardType: "summary_large_image" , }} />
<BaseLayout>
    <Banner />
    <Navigation />
    <Hero />
    <Intro />
    <Features />
    <Team />
    <Vantaggi />
    <Timer />
    <Pricing />
    <Countdown />
    <Footer />
