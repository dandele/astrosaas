---
  import BaseLayout from "../layouts/BaseLayout.astro";
  import Navigation from "../components/global/Navigation.astro";

  import Hero from "../components/landing/Hero.astro";
  import SectionOne from "../components/landing/SectionOne.astro";
  import SectionTwo from "../components/landing/SectionTwo.astro";
  import Testimonial from "../components/global/Testimonial.astro";
  import Pricing from "../components/landing/Pricing.astro";
  import Cta from "../components/global/Cta.astro";
  import Footer from "../components/global/Footer.astro";
  import { AstroSeo } from "@astrolib/seo";
  import FAQ2 from "../components/global/FAQ2.astro";
  import Team from "../components/landing/Team.astro";
  import Saicosa from "../components/landing/Saicosa.astro";
  import team_gallery from "../components/landing/team_gallery.astro";

---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo title="Notion Builders - Home" description="La tua azienda su Notion, in abbonamento"
  canonical="https://www.notionbuilders.it" openGraph={{ url: "https://www.notionbuilders.it" , title: "Notion Builders"
  , description: "La tua azienda su Notion, in abbonamento!" , images: [ {
  url: "https://www.notionbuilders.it/hero.webp" , width: 1200, height: 630, alt: "Social open graph" ,
  type: "image/jpeg" , }, { url: "https://www.notionbuilders.it/hero.webp" , width: 4096, height: 4096,
  alt: "Twitter open graph" , type: "image/jpeg" , }, { url: "https://www.notionbuilders.it/hero.webp" }, {
  url: "https://www.notionbuilders.it/hero.webp" }, ], site_name: "Notion Builders" , }} twitter={{ handle: "" ,
  site: "@site" , cardType: "summary_large_image" , }} />
<BaseLayout>
  <Navigation />
  <Hero />
  <SectionOne />
  <SectionTwo />
  <Testimonial />
  <Team />
  <team_gallery />
  <Saicosa />
  <Pricing />
  <Cta />
  <FAQ2 />
  <Footer />
</BaseLayout>